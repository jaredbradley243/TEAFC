<!-- _includes/header.liquid -->

<header class="bg-white">
  <nav class="mx-auto flex max-w-7xl items-center justify-between p-6 lg:px-8" aria-label="Global">
    <div class="flex lg:flex-1">
      <a href="#" class="-m-1.5 p-1.5">
        <span class="sr-only">The Ed Asner Family Center</span>
        <img
          class="h-8 w-auto"
          src="/assets/img/logo.svg"
          alt="">
      </a>
    </div>
    <div class="flex lg:hidden">
      <button
        id="mobile-menu-open-button"
        type="button"
        class="-m-2.5
        inline-flex
        items-center
        justify-center
        rounded-md
        p-2.5
        text-gray-700">
        <span class="sr-only">Open main menu</span>
        <svg
          class="h-6 w-6"
          fill="none"
          viewBox="0 0 24 24"
          stroke-width="1.5"
          stroke="currentColor"
          aria-hidden="true">
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5" />
        </svg>
      </button>
    </div>
    <div class="hidden lg:flex lg:gap-x-12">
      <a href="#" class="text-sm font-semibold leading-6 text-gray-900 active-nav-link">{{ header.first_link_title
}}</a>
      <div class="relative">
        <button
          type="button"
          class="flex items-center gap-x-1 text-sm font-semibold leading-6 text-gray-900"
          aria-expanded="false">
          {{ header.second_link_title }}
          <svg
            class="h-5 w-5 flex-none text-gray-400 transform"
            viewBox="0 0 20 20"
            fill="currentColor"
            aria-hidden="true">
            <path
              fill-rule="evenodd"
              d="M5.23 7.21a.75.75 0 011.06.02L10 11.168l3.71-3.938a.75.75 0 111.08 1.04l-4.25 4.5a.75.75 0 01-1.08 0l-4.25-4.5a.75.75 0 01.02-1.06z"
              clip-rule="evenodd" />
          </svg>
        </button>

        <!--
          'Product' flyout menu, show/hide based on flyout menu state.
        
          Entering: "transition ease-out duration-200"
          From: "opacity-0 translate-y-1"
          To: "opacity-100 translate-y-0"
          Leaving: "transition ease-in duration-150"
          From: "opacity-100 translate-y-0"
          To: "opacity-0 translate-y-1"
        -->
        <div class="hidden absolute -left-8 top-full z-10 mt-3 w-screen max-w-sm overflow-hidden rounded-3xl bg-white shadow-lg ring-1 ring-gray-900/5">
          <div class="p-4">
            <div class="group relative flex gap-x-6 rounded-lg p-4 text-sm leading-6 hover:bg-gray-50">
              <div class="flex-auto">
                <a href="#" class="block font-semibold text-gray-900">
                  {{ header.second_link_sub_menu_title1 }}
                  <span class="absolute inset-0"></span>
                </a>
                <p class="mt-1 text-gray-600">{{ header.second_link_sub_menu_description }}</p>
              </div>
            </div>
            <div class="group relative flex gap-x-6 rounded-lg p-4 text-sm leading-6 hover:bg-gray-50">
              <div class="flex-auto">
                <a href="#" class="block font-semibold text-gray-900">
                  Our Team
                  <span class="absolute inset-0"></span>
                </a>
                <p class="mt-1 text-gray-600">{{ header.second_link_sub_menu_description2 }}</p>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="relative">
        <button
          type="button"
          class="flex items-center gap-x-1 text-sm font-semibold leading-6 text-gray-900"
          aria-expanded="false">
          {{ header.third_link_title }}
          <svg
            class="h-5 w-5 flex-none text-gray-400 transform"
            viewBox="0 0 20 20"
            fill="currentColor"
            aria-hidden="true">
            <path
              fill-rule="evenodd"
              d="M5.23 7.21a.75.75 0 011.06.02L10 11.168l3.71-3.938a.75.75 0 111.08 1.04l-4.25 4.5a.75.75 0 01-1.08 0l-4.25-4.5a.75.75 0 01.02-1.06z"
              clip-rule="evenodd" />
          </svg>
        </button>

        <!--
          'Product' flyout menu, show/hide based on flyout menu state.
        
          Entering: "transition ease-out duration-200"
          From: "opacity-0 translate-y-1"
          To: "opacity-100 translate-y-0"
          Leaving: "transition ease-in duration-150"
          From: "opacity-100 translate-y-0"
          To: "opacity-0 translate-y-1"
        -->
        <div class="hidden absolute -left-8 top-full z-10 mt-3 w-screen max-w-sm overflow-hidden rounded-3xl bg-white shadow-lg ring-1 ring-gray-900/5">
          <div class="p-4">
            <div class="group relative flex gap-x-6 rounded-lg p-4 text-sm leading-6 hover:bg-gray-50">
              <div class="flex-auto">
                <a href="#" class="block font-semibold text-gray-900">
                  {{ header.third_link_sub_menu_title1 }}
                  <span class="absolute inset-0"></span>
                </a>
                <p class="mt-1 text-gray-600">{{ header.third_link_sub_menu_description1 }}</p>
              </div>
            </div>
            <div class="group relative flex gap-x-6 rounded-lg p-4 text-sm leading-6 hover:bg-gray-50">
              <div class="flex-auto">
                <a href="#" class="block font-semibold text-gray-900">
                  {{ header.third_link_sub_menu_title2 }}
                  <span class="absolute inset-0"></span>
                </a>
                <p class="mt-1 text-gray-600">{{ header.third_link_sub_menu_description2 }}</p>
              </div>
            </div>
          </div>
        </div>
      </div>
      <a href="#" class="text-sm font-semibold leading-6 text-gray-900">{{ header.fourth_link_title }}</a>
      <a href="#" class="text-sm font-semibold leading-6 text-gray-900">{{ header.fifth_link_title }}</a>
      <a href="#" class="text-sm font-semibold leading-6 text-gray-900">{{ header.sixth_link_title }}</a>
      <a href="#" class="text-sm font-semibold leading-6 text-gray-900">{{ header.seventh_link_title }}</a>
      <a href="#" class="text-sm font-semibold leading-6 text-gray-900">{{ header.eighth_link_title }}</a>
    </div>
    <div class="hidden lg:flex lg:flex-1 lg:justify-end"></div>
  </nav>
  <!-- Mobile menu, show/hide based on menu open state. -->
  <div
    id="mobile-menu"
    class="hidden lg:hidden"
    role="dialog"
    aria-modal="true">
    <!-- Background backdrop, show/hide based on slide-over state. -->
    <div class="fixed inset-0 z-10"></div>
    <div class="fixed inset-y-0 right-0 z-10 w-full overflow-y-auto bg-white px-6 py-6 sm:max-w-sm sm:ring-1 sm:ring-gray-900/10">
      <div class="flex items-center justify-between">
        <a href="#" class="-m-1.5 p-1.5">
          <span class="sr-only">The Ed Ansner Family Center</span>
          <img
            class="h-8 w-auto"
            src="/assets/img/logo.svg"
            alt="">
        </a>
        <button
          id="mobile-menu-close-button"
          type="button"
          class="-m-2.5 rounded-md p-2.5 text-gray-700">
          <span class="sr-only">Close menu</span>
          <svg
            class="h-6 w-6"
            fill="none"
            viewBox="0 0 24 24"
            stroke-width="1.5"
            stroke="currentColor"
            aria-hidden="true">
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              d="M6 18L18 6M6 6l12 12" />
          </svg>
        </button>
      </div>
      <div class="mt-6 flow-root">
        <div class="-my-6 divide-y divide-gray-500/10">
          <div class="space-y-2 py-6">
            <a href="#" class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{
header.first_link_title }}</a>
            <div class="-mx-3">
              <button
                type="button"
                class="flex w-full items-center justify-between rounded-lg py-2 pl-3 pr-3.5 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50"
                aria-controls="disclosure-1"
                aria-expanded="false">
                {{ header.second_link_title }}
                <!--
                  Expand/collapse icon, toggle classes based on menu open state.
                
                  Open: "rotate-180", Closed: ""
                -->
                <svg
                  class="h-5 w-5 flex-none transform"
                  viewBox="0 0 20 20"
                  fill="currentColor"
                  aria-hidden="true">
                  <path
                    fill-rule="evenodd"
                    d="M5.23 7.21a.75.75 0 011.06.02L10 11.168l3.71-3.938a.75.75 0 111.08 1.04l-4.25 4.5a.75.75 0 01-1.08 0l-4.25-4.5a.75.75 0 01.02-1.06z"
                    clip-rule="evenodd" />
                </svg>
              </button>
              <!-- 'Product' sub-menu, show/hide based on menu state. -->
              <div class="hidden mt-2 space-y-2" id="disclosure-1">
                <a href="#" class="block rounded-lg py-2 pl-6 pr-3 text-sm font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{
header.second_link_sub_menu_title1 }}</a>
                <a href="#" class="block rounded-lg py-2 pl-6 pr-3 text-sm font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{
header.second_link_sub_menu_title2 }}</a>
              </div>
            </div>

            <div class="-mx-3">
              <button
                type="button"
                class="flex w-full items-center justify-between rounded-lg py-2 pl-3 pr-3.5 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50"
                aria-controls="disclosure-1"
                aria-expanded="false">
                {{ header.third_link_title }}
                <!--
                  Expand/collapse icon, toggle classes based on menu open state.
                
                  Open: "rotate-180", Closed: ""
                -->
                <svg
                  class="h-5 w-5 flex-none transform"
                  viewBox="0 0 20 20"
                  fill="currentColor"
                  aria-hidden="true">
                  <path
                    fill-rule="evenodd"
                    d="M5.23 7.21a.75.75 0 011.06.02L10 11.168l3.71-3.938a.75.75 0 111.08 1.04l-4.25 4.5a.75.75 0 01-1.08 0l-4.25-4.5a.75.75 0 01.02-1.06z"
                    clip-rule="evenodd" />
                </svg>
              </button>
              <!-- 'Product' sub-menu, show/hide based on menu state. -->
              <div class="hidden mt-2 space-y-2" id="disclosure-1">
                <a href="#" class="block rounded-lg py-2 pl-6 pr-3 text-sm font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{
header.third_link_sub_menu_title1 }}</a>
                <a href="#" class="block rounded-lg py-2 pl-6 pr-3 text-sm font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{
header.third_link_sub_menu_title2 }}</a>
              </div>
            </div>

            <a href="#" class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{
header.fourth_link_title }}</a>
            <a href="#" class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{
header.fifth_link_title }}</a>
            <a href="#" class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{
header.sixth_link_title }}</a>
            <a href="#" class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{
header.seventh_link_title }}</a>
            <a href="#" class="-mx-3 block rounded-lg px-3 py-2 text-base font-semibold leading-7 text-gray-900 hover:bg-gray-50">{{
header.eighth_link_title }}</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>